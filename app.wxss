/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page {
    background: #f6f6f6;
    padding: 0;
    margin: 0;
}

.special {
    font-size: 36rpx;
    text-align: center;
    background: linear-gradient(to bottom, rgba(255, 255, 255, 1) 0%, rgba(255, 255, 255, 1) 49%, rgba(255, 234, 226, 1) 52%, rgba(255, 220, 207, 1) 100%);
}
